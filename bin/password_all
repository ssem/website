#!/bin/bash
DIR="/home/ssem/passwords"

function run {
    echo $1
    ./password_hashcat ${DIR}/$1 $2
}

run abc sha1
run battlefield md5
run blackstar.md5 md5
run blackstar.sha1 sha1
run blackstar.mysql mysql
run casio md5
run damnsmalllinux md5
run dhool md5
